{
  "version": 2,
  "routes": [
    {
      "src": "/sendmail/(.*)",
      "dest": "https://api.mailersend.com/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/dingtalk/(.*)",
      "dest": "https://oapi.dingtalk.com/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    }
  ]
}